[user]
    name = Reed Weichler
    email = rweichler@gmail.com
[color]
    ui = auto
[alias]
    count = rev-list HEAD --count
    gay = status
    ls = status
    day = log --since="yesterday" --pretty="oneline"
    pushm = push origin master
    pullm = pull origin master
    pusho = push origin
    pullo = pull origin
    lg = log --oneline --decorate --all --graph -30
    goback = reset --hard HEAD~1
    forcepush = push origin HEAD --force
[core]
	excludesfile = ~/.gitignore_global
